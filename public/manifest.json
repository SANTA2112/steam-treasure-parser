{
  "name": "Steam Treasure Parser",
  "description": "Parsing items price in treasure",
  "version": "1.0.0",
  "manifest_version": 2,
  "author": "SANTA2112",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.steamcommunity.com/market/listings/*"],
      "js": ["contentscript.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["script.js"]
}
